        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "minimum": 1
          },
          "email": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "enum": [
              "active",
              "suspended"
            ]
          }
        },
        "required": [
          "id"
        ]
      },
      "CreditUserDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
